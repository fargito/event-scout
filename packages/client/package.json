{
  "name": "@event-scout/client",
  "description": "EventScout client",
  "version": "0.3.0",
  "author": "fargito",
  "license": "MIT",
  "homepage": "https://github.com/fargito/event-scout",
  "bugs": "https://github.com/fargito/event-scout/issues",
  "repository": "fargito/event-scout.git",
  "keywords": [
    "CDK",
    "CDK Constructs",
    "EventBridge",
    "Serverless"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "package": "pnpm clean && pnpm package-transpile && pnpm package-types && pnpm package-types-aliases",
    "package-transpile": "tsup",
    "package-types": "tsc -p tsconfig.build.json",
    "package-types-aliases": "tsc-alias -p tsconfig.build.json",
    "test": "pnpm test-linter && pnpm test-type && pnpm test-unit && pnpm test-circular",
    "test-circular": "pnpm depcruise --config -- .",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc --noEmit --emitDeclarationOnly false",
    "test-unit": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^3.0.0",
    "@aws-sdk/signature-v4": "^3.257.0",
    "@aws-sdk/types": "^3.257.0",
    "@event-scout/construct-contracts": "^0.3.0",
    "@swarmion/serverless-contracts": "^0.23.1",
    "axios": "^1.3.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.19",
    "@vitest/coverage-c8": "^0.28.4",
    "concurrently": "^7.6.0",
    "dependency-cruiser": "^12.7.0",
    "eslint": "^8.33.0",
    "json-schema-to-ts": "^2.6.2",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4"
  }
}
