{
  "name": "event-scout",
  "description": "EventScout CLI",
  "version": "0.3.0",
  "author": "fargito",
  "license": "MIT",
  "homepage": "https://github.com/fargito/event-scout",
  "bugs": "https://github.com/fargito/event-scout/issues",
  "repository": "fargito/event-scout.git",
  "keywords": [
    "CDK",
    "CDK Constructs",
    "EventBridge",
    "Serverless"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "ncc build ./src/index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "clean": "rimraf dist",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "test": "pnpm test-linter && pnpm test-type && pnpm test-unit && pnpm test-circular",
    "test-circular": "pnpm depcruise --config -- .",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc --noEmit --emitDeclarationOnly false",
    "test-unit": "vitest run --coverage --passWithNoTests"
  },
  "bin": "./dist/index.js",
  "dependencies": {
    "@aws-crypto/sha256-js": "^3.0.0",
    "@aws-sdk/credential-providers": "^3.267.0",
    "@aws-sdk/signature-v4": "^3.267.0",
    "@aws-sdk/types": "^3.267.0",
    "@event-scout/construct-contracts": "workspace:^",
    "ajv": "^8.12.0",
    "commander": "^10.0.0",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/ws": "^8.5.4",
    "@vercel/ncc": "^0.36.1",
    "@vitest/coverage-c8": "^0.28.4",
    "concurrently": "^7.6.0",
    "dependency-cruiser": "^12.7.1",
    "eslint": "^8.34.0",
    "json-schema-to-ts": "^2.6.2",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4"
  }
}
