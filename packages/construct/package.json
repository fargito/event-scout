{
  "name": "@event-scout/construct",
  "description": "EventScout: construct to ease EventBridge monitoring and testing",
  "version": "0.3.0",
  "author": "fargito",
  "license": "MIT",
  "homepage": "https://github.com/fargito/event-scout",
  "bugs": "https://github.com/fargito/event-scout/issues",
  "repository": "fargito/event-scout.git",
  "keywords": [
    "CDK",
    "CDK Constructs",
    "EventBridge",
    "Serverless"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "package": "pnpm package-transpile && pnpm package-types && pnpm package-types-aliases",
    "package-transpile": "tsup",
    "package-types": "tsc -p tsconfig.build.json",
    "package-types-aliases": "tsc-alias -p tsconfig.build.json",
    "test": "pnpm test-linter && pnpm test-type && pnpm test-unit && pnpm test-circular",
    "test-circular": "pnpm depcruise --config -- .",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc --noEmit --emitDeclarationOnly false",
    "test-unit": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.64.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "2.64.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.64.0-alpha.0",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.267.0",
    "@aws-sdk/client-eventbridge": "^3.267.0",
    "@event-scout/construct-contracts": "^0.3.0",
    "@swarmion/serverless-contracts": "^0.24.2",
    "@swarmion/serverless-helpers": "^0.24.2",
    "ajv": "^8.12.0",
    "aws-cdk-lib": "^2.64.0",
    "aws-sdk": "^2.1313.0",
    "constructs": "^10.1.246"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.110",
    "@types/node": "^18.13.0",
    "@vitest/coverage-c8": "^0.28.4",
    "concurrently": "^7.6.0",
    "dependency-cruiser": "^12.7.1",
    "eslint": "^8.34.0",
    "json-schema-to-ts": "^2.6.2",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsup": "^6.6.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4"
  }
}
