{
  "name": "@event-scout/construct",
  "description": "EventScout: construct to ease EventBridge monitoring and testing",
  "version": "0.4.0",
  "author": "fargito",
  "license": "MIT",
  "homepage": "https://github.com/fargito/event-scout",
  "bugs": "https://github.com/fargito/event-scout/issues",
  "repository": "fargito/event-scout.git",
  "keywords": [
    "CDK",
    "CDK Constructs",
    "EventBridge",
    "Serverless"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "package": "pnpm package-transpile && pnpm package-types && pnpm package-types-aliases",
    "package-transpile": "tsup",
    "package-types": "tsc -p tsconfig.build.json",
    "package-types-aliases": "tsc-alias -p tsconfig.build.json",
    "test": "pnpm test-linter && pnpm test-type && pnpm test-unit && pnpm test-circular",
    "test-circular": "pnpm depcruise --config -- .",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc --noEmit --emitDeclarationOnly false",
    "test-unit": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.110.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "2.110.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.110.0-alpha.0",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.454.0",
    "@aws-sdk/client-eventbridge": "^3.454.0",
    "@event-scout/construct-contracts": "^0.4.0",
    "@swarmion/serverless-contracts": "^0.29.0",
    "@swarmion/serverless-helpers": "^0.30.0",
    "ajv": "^8.12.0",
    "aws-cdk-lib": "^2.110.0",
    "aws-sdk": "^2.1499.0",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.126",
    "@types/node": "^20.9.1",
    "@vitest/coverage-v8": "^0.34.6",
    "concurrently": "^8.2.2",
    "dependency-cruiser": "^15.3.0",
    "eslint": "^8.54.0",
    "json-schema-to-ts": "^2.9.2",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.8",
    "tsup": "^7.3.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  }
}
