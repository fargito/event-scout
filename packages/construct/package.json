{
  "name": "@event-scout/construct",
  "description": "EventScout: construct to ease EventBridge monitoring and testing",
  "version": "0.6.1",
  "author": "fargito",
  "license": "MIT",
  "homepage": "https://github.com/fargito/event-scout",
  "bugs": "https://github.com/fargito/event-scout/issues",
  "repository": "fargito/event-scout.git",
  "keywords": [
    "CDK",
    "CDK Constructs",
    "EventBridge",
    "Serverless"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "package": "pnpm package-transpile && pnpm package-bundle && pnpm package-types && pnpm package-types-aliases",
    "package-bundle": "node ./esbuild.build.js",
    "package-transpile": "tsup",
    "package-types": "tsc -p tsconfig.build.json",
    "package-types-aliases": "tsc-alias -p tsconfig.build.json",
    "test": "pnpm test-linter && pnpm test-type && pnpm test-unit && pnpm test-circular",
    "test-circular": "pnpm depcruise --config -- .",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc --noEmit --emitDeclarationOnly false",
    "test-unit": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.490.0",
    "@aws-sdk/client-dynamodb": "^3.564.0",
    "@aws-sdk/client-eventbridge": "^3.490.0",
    "@aws-sdk/lib-dynamodb": "^3.564.0",
    "@event-scout/construct-contracts": "workspace:^0.6.1",
    "@swarmion/serverless-contracts": "^0.31.0",
    "@swarmion/serverless-helpers": "^0.31.0",
    "ajv": "^8.12.0",
    "aws-cdk-lib": "^2.121.1",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.131",
    "@types/node": "^20.11.4",
    "@vitest/coverage-v8": "^1.2.0",
    "concurrently": "^8.2.2",
    "dependency-cruiser": "^16.0.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "json-schema-to-ts": "^3.0.0",
    "prettier": "^3.2.2",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.8",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.2.0"
  }
}
