{
  "name": "@event-scout/construct",
  "description": "EventScout: construct to ease EventBridge monitoring and testing",
  "version": "0.1.7",
  "author": "fargito",
  "license": "MIT",
  "homepage": "https://github.com/fargito/event-scout",
  "bugs": "https://github.com/fargito/event-scout/issues",
  "repository": "fargito/event-scout.git",
  "keywords": [
    "CDK",
    "CDK Constructs",
    "EventBridge",
    "Serverless"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "package": "pnpm clean && pnpm package-cjs && pnpm package-esm && pnpm package-types && pnpm package-types-aliases",
    "package-cjs": "NODE_ENV=cjs pnpm transpile --out-dir dist/cjs --source-maps",
    "package-esm": "NODE_ENV=esm pnpm transpile --out-dir dist/esm --source-maps",
    "package-types": "tsc -p tsconfig.build.json",
    "package-types-aliases": "tsc-alias -p tsconfig.build.json",
    "test": "pnpm test-linter && pnpm test-type && pnpm test-unit && pnpm test-circular",
    "test-circular": "pnpm depcruise --validate .dependency-cruiser.js src",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc --noEmit --emitDeclarationOnly false",
    "test-unit": "vitest run --coverage --passWithNoTests",
    "transpile": "babel src --extensions .ts --quiet",
    "watch": "pnpm clean dist && concurrently 'pnpm:package-* --watch'"
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.241.0",
    "@babel/runtime": "^7.20.7",
    "@event-scout/construct-contracts": "^0.1.7",
    "@swarmion/serverless-contracts": "^0.17.0",
    "@swarmion/serverless-helpers": "^0.17.0",
    "aws-cdk-lib": "^2.59.0",
    "aws-sdk": "^2.1288.0",
    "constructs": "^10.1.210"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/aws-lambda": "^8.10.109",
    "@types/node": "^18.11.18",
    "@vitest/coverage-c8": "^0.25.8",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^7.6.0",
    "dependency-cruiser": "^12.3.0",
    "eslint": "^8.31.0",
    "json-schema-to-ts": "^2.6.2",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.25.8"
  }
}
