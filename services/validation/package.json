{
  "name": "@event-scout/validation",
  "private": true,
  "version": "0.3.0",
  "license": "UNLICENSED",
  "scripts": {
    "bootstrap": "cdk bootstrap --profile event-scout-developer",
    "bootstrap-production": "cdk bootstrap --context stage=production",
    "bootstrap-staging": "cdk bootstrap --context stage=staging",
    "deploy": "cdk deploy --profile event-scout-developer",
    "deploy-production": "cdk deploy --context stage=production",
    "deploy-staging": "cdk deploy --context stage=staging",
    "destroy": "cdk destroy --profile event-scout-developer",
    "destroy-production": "cdk destroy --context stage=production",
    "destroy-staging": "cdk destroy --context stage=staging",
    "generate-tezos-address": "ts-node scripts/generateTezosAddress.ts",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "test": "pnpm test-linter && pnpm test-type && pnpm test-unit && pnpm test-circular",
    "test-circular": "pnpm depcruise --validate .dependency-cruiser.js .",
    "test-integration": "vitest run --coverage --passWithNoTests --config vite.config-integration.ts",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc --noEmit --emitDeclarationOnly false",
    "test-unit": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.264.0",
    "@aws-sdk/client-eventbridge": "^3.264.0",
    "@aws-sdk/client-ssm": "^3.264.0",
    "@aws-sdk/credential-providers": "^3.264.0",
    "@aws-sdk/types": "^3.257.0",
    "@event-scout/client": "workspace:^",
    "@event-scout/construct": "workspace:^"
  },
  "devDependencies": {
    "@event-scout/serverless-configuration": "workspace:^",
    "@swarmion/serverless-helpers": "^0.23.1",
    "@types/node": "^18.11.19",
    "aws-cdk": "^2.63.2",
    "aws-cdk-lib": "^2.63.2",
    "constructs": "^10.1.242",
    "dependency-cruiser": "^12.7.0",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "json-schema-to-ts": "^2.6.2",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4"
  }
}
